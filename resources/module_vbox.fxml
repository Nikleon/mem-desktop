<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="BOTTOM_CENTER" spacing="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mem.gui.ModuleController">
   <children>
      <HBox alignment="TOP_CENTER">
         <children>
            <VBox fx:id="parent" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0">
               <children>
                  <ToggleButton fx:id="chromeToggle" focusTraversable="false" mnemonicParsing="false" text="Chrome Monitor" />
                  <ToggleButton fx:id="desktopToggle" focusTraversable="false" mnemonicParsing="false" text="Desktop Monitor" />
                  <ToggleButton fx:id="calendarToggle" focusTraversable="false" mnemonicParsing="false" text="Google Calendar" />
               </children>
            </VBox>
         </children>
      </HBox>
      <VBox alignment="CENTER" minWidth="180.0" prefHeight="180.0">
         <children>
            <Label fx:id="name" alignment="CENTER" text="USER: Nikleon">
               <font>
                  <Font name="Agency FB Bold" size="36.0" />
               </font>
            </Label>
            <Hyperlink fx:id="logoutLink" focusTraversable="false" text="LOG OUT">
               <font>
                  <Font name="Calibri" size="14.0" />
               </font>
            </Hyperlink>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
